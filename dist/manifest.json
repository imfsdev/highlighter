{
  "manifest_version": 2,
  "name": "Y2Highlighter",
  "description": "This plugin will highlight specified elements on specific urls.",
  "version": "1.0",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/128.png",
    "default_popup": "popup.html",
    "default_title": "Y2Highlighter"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": ["js/background.js"]
  }
}
